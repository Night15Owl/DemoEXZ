<Window x:Class="DExz.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DExz"
        mc:Ignorable="d"
        Title="Автодилер" MinHeight="550" MinWidth="800">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="25*"/>
            <RowDefinition Height="200*"/>
        </Grid.RowDefinitions>

        <Grid Grid.Row="0">
            <Button x:Name="btnBack" Content="Назад" Style="{StaticResource btn}"
                    HorizontalAlignment="Right" Click="btnBack_Click"></Button>
        </Grid>

        <Grid Grid.Row="1">
            
        </Grid>

        <Frame Grid.Row="1" x:Name="FrmMain" NavigationUIVisibility="Hidden" ContentRendered="FrmMain_ContentRendered"></Frame>
    </Grid>
</Window>
